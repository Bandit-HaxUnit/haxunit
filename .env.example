# HaxUnit Environment Variables Configuration
# Copy this file to .env and fill in your API keys

# ProjectDiscovery Cloud Platform API Key
# Get your API key from: https://cloud.projectdiscovery.io
PDCP_API_KEY=your_pdcp_api_key_here

# WPScan API Key
# Get your API key from: https://wpscan.com/api
WPSCAN_API_KEY=your_wpscan_api_key_here

# Acunetix API Key (if using Acunetix integration)
ACUNETIX_API_KEY=your_acunetix_api_key_here

# HaxUnit API Key (for custom features)
HAXUNIT_API_KEY=your_haxunit_api_key_here

ACUNETIX_THRESHOLD=30
NUCLEI_API_KEY=
HTB_OPENVPN_FILE=